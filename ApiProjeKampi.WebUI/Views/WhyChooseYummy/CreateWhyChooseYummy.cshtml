@model CreateWhyChooseYummyDto

@{
    ViewData["Title"] = "CreateWhyChooseYummy";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}



<section class="section">
    <div class="section-body">
        <div class="row">
            <div class="col-12 col-md-12 col-lg-12">
                <div class="card">
                    <div class="card-header">
                        <h4>Neden Biz Ekleme Sayfası</h4>
                    </div>
                    <div class="card-body">
                        <form method="post">
                            <label>Başlık</label>
                            <input type="text" asp-for="Title" class="form-control" />
                            <br />

                            <label>Açıklama</label>
                            <textarea class="form-control" asp-for="Description" rows="5"></textarea>
                            <br />

                            <label>Ikon Url</label>
                            <input type="text" asp-for="IconUrl" class="form-control" />
                            <br />



                            <button class="btn btn-primary">  Oluştur</button>
                        </form>

                    </div>
                </div>



            </div>

        </div>
    </div>
</section>


